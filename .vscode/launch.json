    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "Sources.dbextractor",
            "justMyCode": false,
            "args": [
                "false",
                "true",
                "true"
            ]
        },
        {
            "name": "Python: Scripting Tools",
            "type": "python",
            "request": "launch",
            "module": "Sources.ScriptingTools.copytool",
            "justMyCode": false,
            "args": [
                "silextract",
                "Sources/.cache/images",
                "Sources/.cache/extracted_silhouettes"
            ]
        },
        {
            "name": "Python: DB Analyser",
            "type": "python",
            "request": "launch",
            "module": "Sources.ScriptingTools.dbanalyser",
            "justMyCode": false,
            "args": [
                "Sources/.cache/deployed/recipes/all_recipes.json",
                "Sources/.cache/deployed/dbanalysis"
            ]
        },
        {
            "name": "Python: DB Sanitizer",
            "type": "python",
            "request": "launch",
            "module": "Sources.dbsanitizer",
            "justMyCode": false,
            "args": [
                "References",
                "Sources/.cache/deployed",
                "Sources/.cache/deployed/dbsanitizer"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}